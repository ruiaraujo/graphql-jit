{
  "name": "@hoangvvo/graphql-jit",
  "version": "0.5.0",
  "description": "GraphQL JIT Compiler to JS",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Hoang Vo (https://hoangvvo.com)",
  "keywords": [
    "graphql",
    "graphql-js",
    "jit",
    "performance"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:hoangvvo/graphql-jit.git"
  },
  "scripts": {
    "prepublishOnly": "yarn && yarn build",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "build": "tsc",
    "test": "jest",
    "test:ci": "jest --ci --coverage && codecov",
    "benchmark": "NODE_ENV=production ts-node -T ./src/__benchmarks__/benchmarks.ts"
  },
  "files": [
    "dist/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "testRegex": "(/tests/.*|(\\.|/)test)\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 94,
        "functions": 96,
        "lines": 96,
        "statements": 96
      }
    }
  },
  "peerDependencies": {
    "graphql": ">=15"
  },
  "devDependencies": {
    "@graphql-tools/schema": "^7.1.3",
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.20",
    "@types/lodash.memoize": "^4.1.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.mergewith": "^4.6.6",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "benchmark": "^2.1.4",
    "codecov": "^3.8.1",
    "fast-json-stringify": "^2.4.2",
    "graphql": "^15.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "generate-function": "^2.3.1",
    "lodash.memoize": "^4.1.2",
    "lodash.merge": "4.6.2",
    "lodash.mergewith": "4.6.2"
  }
}
